@model P_Scanner.Models.UserInput

<h2>Generate QR Code</h2>

<form asp-action="Generate" asp-controller="QRCode" method="post">
  <label for="Variable1">Numero du Siege:</label>
  <input type="text" id="Variable1" name="Variable1" /><br />

  <label for="Variable2">Numero du client:</label>
  <input type="text" id="Variable2" name="Variable2" /><br />

  <label for="Variable3">Numero du Film :</label>
